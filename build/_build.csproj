<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<OutputType>Exe</OutputType>
		<TargetFramework>net6.0</TargetFramework>
		<IsPackable>false</IsPackable>
		<RootNamespace></RootNamespace>
		<NukeRootDirectory>..</NukeRootDirectory>
		<NukeScriptDirectory>..</NukeScriptDirectory>
		<NukeTelemetryOption>1</NukeTelemetryOption>
		<IsPackable>False</IsPackable>
		<NoWarn>CS0649;CS0169</NoWarn>
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="Nuke.Common" Version="6.0.1" />
		<PackageReference Include="Nuke.Components" Version="6.0.1" />
		<PackageDownload Include="GitVersion.Tool" Version="[5.8.0]" />
		<PackageDownload Include="xunit.runner.console" Version="[2.4.1]" />
	</ItemGroup>
</Project>
