<?xml version="1.0" encoding="utf-8"?>
<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
	<PropertyGroup>
		<_AspNetCoreClientGeneratorTaskAssembly>.\AspNetCore.Client.Generator.dll</_AspNetCoreClientGeneratorTaskAssembly>
	</PropertyGroup>

	<PropertyGroup>
		<GenerateClients>true</GenerateClients>
		<RouteToServiceProjectFolder></RouteToServiceProjectFolder>
		<ClientInterfaceName>MyServiceClient</ClientInterfaceName>
		<UseValueTask>true</UseValueTask>
		<ClientNamespace>MyService.Clients</ClientNamespace>
		<BlazorClients>false</BlazorClients>
		<AllowedNamespaces>System*;</AllowedNamespaces>
		<ExcludedNamespaces></ExcludedNamespaces>
	</PropertyGroup>

	<UsingTask AssemblyFile="$(_AspNetCoreClientGeneratorTaskAssembly)" TaskName="AspNetCore.Client.Generator.GeneratorTask"/>
</Project>