		<PackageReference Include="Microsoft.AspNetCore.TestHost" Version="2.1.1" />
		<PackageReference Include="Microsoft.NET.Test.Sdk" Version="15.9.0" />
		<PackageReference Include="Moq" Version="4.10.0" />
		<PackageReference Include="NUnit" Version="3.11.0" />
		<PackageReference Include="NUnit3TestAdapter" Version="3.10.0" />
	</ItemGroup>

	<ItemGroup>
		<ProjectReference Include="..\..\src\AspNetCore.Client.Protobuf\AspNetCore.Client.Protobuf.csproj" />
		<ProjectReference Include="..\TestWebApp.Clients\TestWebApp.Clients.csproj" />
		<ProjectReference Include="..\TestWebApp\TestWebApp.csproj" />
	</ItemGroup>

</Project>
